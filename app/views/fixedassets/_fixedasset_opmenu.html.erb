    <% if fixedasset.status == "in_use" or fixedasset.status == "in_use_extend" or fixedasset.status == "depreciated_done" %>
      <%= link_to '轉移', new_fixedasset_transfer_path(fixedasset.id), :class => "btn btn-primary btn-sm"%>
      <%= link_to '報廢', new_fixedasset_depreciate_path(fixedasset.id), :class => "btn btn-primary btn-sm"%>
      <%= link_to '出售', new_fixedasset_sell_path(fixedasset.id), :class => "btn btn-primary btn-sm"%>
    <% end %>